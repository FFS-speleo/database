    "type": "object",
    "required": ["id", "email", "password"],
    "properties": {
      "id": {
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "password": {
        "type": "string"
      }
    },
    "additionalProperties": false
  }
}