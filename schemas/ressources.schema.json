{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Array of Ressource",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["id", "title", "description", "page", "url"],
    "properties": {
      "id": {
        "type": "string"
      },
      "title": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "page": {
        "type": "integer",
        "minimum": 1
      },
      "url": {
        "type": "string",
        "format": "uri"
      }
    },
    "additionalProperties": false
  }
}